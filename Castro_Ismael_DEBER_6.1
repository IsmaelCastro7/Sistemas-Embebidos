void setup() {
size(1000, 700);
}
void draw(){
  background(0);
  fill(255,255,0);
  ellipse(400,400,400,400);
  fill(0);
  ellipse(400,300,50,50);
  fill(0);
  arc(400,400,400,400,5.7,7.4);
  fill(255);
  noStroke();
  
  ellipse(540,450,60,60);
  fill(255);
  noStroke();
  ellipse(640,450,60,60);
  fill(255);
  noStroke();
  ellipse(740,450,60,60);
  fill(255);
  noStroke();
  ellipse(840,450,50,50);
}


