void setup() {
size(500, 500);
}
void draw(){
  background(0);
  fill(#ff8000);
  stroke(255,128,0);
  rect(20,20,450,450);
  fill(#ffffff);
  stroke(255,255,255);
  ellipse(250,250,380,380);
  fill(#639a2b);
  stroke(0,255,0);
  ellipse(250,250,280,280);
  fill(#cc0000);
  stroke(0,255,0);
  ellipse(250,250,275,275);
  stroke(0,0,255);
  strokeWeight(5);
  line(20,20,113,113);
  line(20,470,113,387);
  line(470,20,387,113);
  line(470,470,387,387);
}
